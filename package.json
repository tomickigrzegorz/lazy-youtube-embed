{
  "name": "youtubelazy",
  "version": "1.0.4",
  "description": "Lazy loading youtube on lightbox",
  "main": "docs/youtubeLazy.min.js",
  "scripts": {
    "dev": "concurrently \"yarn sass:prod\" \"yarn sass:dev\" \"yarn watch\"",
    "prod": "concurrently \"yarn build\" \"yarn sass:prod\" \"yarn postcss\"",
    "build": "rollup --config --environment PRODUCTION",
    "watch": "rollup --config --watch",
    "sass:dev": "node-sass ./sources/style.scss ./docs/youtubeLazy.min.css -w",
    "sass:prod": "node-sass ./sources/style.scss ./docs/youtubeLazy.min.css --output-style compressed",
    "postcss": "postcss docs/youtubeLazy.min.css -p sugarss -o docs/youtubeLazy.min.css -m"
  },
  "author": "Grzegorz Tomicki",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-babel": "^5.2.1",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "postcss-cli": "^8.0.0",
    "postcss-css-variables": "^0.17.0",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2"
  }
}