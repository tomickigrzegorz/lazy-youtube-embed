{
  "name": "youtubelazy",
  "version": "1.1.6",
  "description": "Lazy loading youtube",
  "author": "Grzegorz Tomicki",
  "license": "MIT",
  "main": "dist/js/youtubeLazy.js",
  "scripts": {
    "dev": "concurrently \"yarn sass:dev\" \"yarn sass:prod\" \"yarn watch\"",
    "prod": "yarn build && yarn sass:prod && yarn postcss:global && yarn postcss:lazyYoutube && yarn postcss:ie && yarn lib:version",
    "build": "rollup -c --environment PRODUCTION --bundleConfigAsCjs",
    "watch": "rollup -c -w --bundleConfigAsCjs",
    "sass:prod": "sass --no-source-map sources/scss:docs/css",
    "sass:dev": "sass --watch sources/scss:docs/css",
    "postcss:global": "postcss docs/css --dir docs/css",
    "postcss:lazyYoutube": "postcss --ext min.css docs/css/youtubeLazy.css --dir dist/css/",
    "postcss:ie": "postcss --u postcss-css-variables --no-map --ext ie.min.css docs/css/youtubeLazy.css --dir dist/css/",
    "lib:version": "node ./scripts/version.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.20",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.1.4",
    "concurrently": "^8.2.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-css-variables": "^0.19.0",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.0.3",
    "rollup": "^3.29.5",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.68.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
