{
  "name": "youtubelazy",
  "version": "1.0.1",
  "description": "",
  "main": "docs/youtubeLazy.min.js",
  "scripts": {
    "dev": "concurrently \"yarn sass:prod\" \"yarn sass:dev\" \"yarn watch\"",
    "prod": "concurrently \"yarn build\" \"yarn sass:prod\" \"yarn postcss\"",
    "build": "rollup --config --environment PRODUCTION",
    "watch": "rollup --config --watch",
    "sass:dev": "node-sass ./sources/style.scss ./docs/youtubeLazy.min.css -w",
    "sass:prod": "node-sass ./sources/style.scss ./docs/youtubeLazy.min.css --output-style compressed",
    "postcss": "postcss docs/youtubeLazy.min.css -p sugarss -o docs/youtubeLazy.min.css -m"
  },
  "author": "Grzegorz Tomicki",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.2.0",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1",
    "postcss-css-variables": "^0.17.0",
    "prettier": "^2.0.5",
    "promise-polyfill": "^8.1.3",
    "rollup": "^2.25.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0"
  }
}